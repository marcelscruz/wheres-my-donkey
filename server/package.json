  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.6.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3"
  }
}
